              <se:SvgParameter name="font-size">10</se:SvgParameter>
            </se:Font>
            <se:Fill>
              <se:SvgParameter name="fill">#0000FF</se:SvgParameter>
            </se:Fill>
          </se:TextSymbolizer>
        </se:Rule>
